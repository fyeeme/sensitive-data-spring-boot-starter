{
  "groups": [
    {
      "name": "sensitive.log",
      "type": "com.example.sensitive.config.SensitiveLogProperties",
      "description": "敏感日志脱敏配置"
    },
    {
      "name": "sensitive.log.to-string-config",
      "type": "com.example.sensitive.config.SensitiveLogProperties$ToStringConfig",
      "description": "toString 脱敏配置"
    }
  ],
  "properties": [
    {
      "name": "sensitive.log.enabled",
      "type": "java.lang.Boolean",
      "description": "是否启用脱敏功能",
      "defaultValue": true
    },
    {
      "name": "sensitive.log.mask-char",
      "type": "java.lang.Character",
      "description": "默认掩码字符",
      "defaultValue": "*"
    },
    {
      "name": "sensitive.log.enable-logging-mapper",
      "type": "java.lang.Boolean",
      "description": "是否启用日志专用 ObjectMapper",
      "defaultValue": true
    },
    {
      "name": "sensitive.log.to-string-config.enable-cache",
      "type": "java.lang.Boolean",
      "description": "是否启用反射缓存",
      "defaultValue": true
    },
    {
      "name": "sensitive.log.to-string-config.cache-max-size",
      "type": "java.lang.Integer",
      "description": "缓存最大容量",
      "defaultValue": 1000
    }
  ],
  "hints": []
}
